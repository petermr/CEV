    <th align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">
     <sup>a</sup> CC
     <sub>50</sub>
    </th>
    <th align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">
     <sup>b</sup> EC
     <sub>50</sub>
    </th>
   </tr>
  </thead>
  <tbody>
   <tr>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">* Salvia officinalis</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">&gt; 100</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">50 ± 3</td>
   </tr>
