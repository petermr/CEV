<?xml version="1.0" encoding="UTF-8"?>
<glossary>
 <title>Abbreviations</title>
 <def-list>
  <def-item>
   <term>ATC:</term>
   <def>
    <p>American Type Culture Collection used as reference strains for respective bacteria</p>
   </def>
  </def-item>
  <def-item>
   <term>BA:</term>
   <def>
    <p>Blood agar</p>
   </def>
  </def-item>
  <def-item>
   <term>CLSI:</term>
   <def>
    <p>Clinical Laboratory Standard Institute</p>
   </def>
  </def-item>
  <def-item>
   <term>DDST:</term>
   <def>
    <p>Double disk synergy test</p>
   </def>
  </def-item>
  <def-item>
   <term>EOs:</term>
   <def>
    <p>Essential oils obtained from the respective medicinal plant by steam distillation</p>
   </def>
  </def-item>
  <def-item>
   <term>ESBL:</term>
   <def>
    <p>Extended-spectrum beta-lactamase</p>
   </def>
  </def-item>
  <def-item>
   <term>FIC:</term>
   <def>
    <p>Fractional inhibitory concentration index</p>
   </def>
  </def-item>
  <def-item>
   <term>MBC:</term>
   <def>
    <p>Minimum bactericidal concentration</p>
   </def>
  </def-item>
  <def-item>
